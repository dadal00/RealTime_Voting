    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "d3": "^7.9.0",
    "svelte-visibility-change": "^0.7.0"
  },
  "overrides": {
    "cookie": "0.7.0"
  }
